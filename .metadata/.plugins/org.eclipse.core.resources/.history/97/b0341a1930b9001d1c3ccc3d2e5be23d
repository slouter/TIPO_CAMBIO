package com.dev.cambio.model;

import java.util.Objects;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "tipoCambio")
public class TipoCambio {
	
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	@Column(name = "indentificador")
	private int x_ident;
	
	@Column(name = "desripcion")
	private String x_desc;
	
	@Column(name = "estado")
	private float x_estado;
	
	@Column(name = "intervalo_1")
	private float x_inter1;
	
	@Column(name = "tccompra_1")
	private float x_tccompra1;
	
	@Column(name = "tcventa_1")
	private float x_tcventa1;
	
	@Column(name = "intervalo_2")
	private float x_inter2;
	
	@Column(name = "tccompra_2")
	private float x_tccompra2;
	
	@Column(name = "tcventa_2")
	private float x_tcventa2;
	
	@Column(name = "intervalo_3")
	private float x_inter3;
	
	@Column(name = "tccompra_3")
	private float x_tccompra3;
	
	@Column(name = "tcventa_3")
	private float x_tcventa3;
	
	@Column(name = "intervalo_4")
	private float x_inter4;
	
	@Column(name = "tccompra_4")
	private float x_tccompra4;
	
	@Column(name = "tcventa_4")
	private float x_tcventa4;
	
	@Column(name = "intervalo_5")
	private float x_inter5;
	
	@Column(name = "tccompra_5")
	private float x_tccompra5;
	
	@Column(name = "tcventa_5")
	private float x_tcventa5;

	public TipoCambio() {		
	}

		
}
